{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": [
            true,
            "match-declaration-order"
        ],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "case-insensitive",
                "named-imports-order": "case-insensitive"
            }
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ]
    },
    "rulesDirectory": []
}